<div class="content">
  <div id="search-box">
    <%= form_with(url: '/', method: :get) do |f| %>
      <%= text_field :title, "", id: "title_search", 
      name: "keyword", placeholder: "キーワードを打ち込んでください", 
      style: "width: 200px;" %><button title="検索" type="submit">検索</button>
    <% end %>
  </div>

  <h2>検索結果</h2>
  <div id="search_list">
     <%= render partial: "search_list" %>
  </div>
</div>